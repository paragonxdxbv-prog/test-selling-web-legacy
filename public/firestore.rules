rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Public read for products and settings (rules). No writes from public.
    match /products/{productId} {
      allow read: if true;
      allow write: if false;
    }
    match /settings/{docId} {
      allow read: if true;
      allow write: if false;
    }
  }
}
